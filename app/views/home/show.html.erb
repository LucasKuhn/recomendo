<script>
const isIos = /iphone|ipad|ipod/.test(window.navigator.userAgent.toLowerCase());
const isMobileDevice = /Mobi/i.test(window.navigator.userAgent);
const isInStandaloneMode = (window.navigator.standalone) || (window.matchMedia('(display-mode: standalone)').matches); // https://web.dev/customize-install/

if ( isMobileDevice ) {
  if ( isInStandaloneMode ) {
    document.querySelector('#login-button').classList.toggle('d-none');
  }
  else if ( isIos ) {
    const isSafari = !(/(CriOS|FxiOS|OPiOS|mercury)/i.test(window.navigator.userAgent))
    if ( isSafari ) {
      document.querySelector('#apple-button').classList.toggle('d-none');
    } else {

    }
  } else {
    document.querySelector('#mobile-button').classList.toggle('d-none');
  }
} else {
  document.querySelector('#login-button').classList.toggle('d-none');
}


// Button to trigger PWA on Android
btnInstallApp = document.querySelector('#mobile-button')


// PWA Triggering
let deferredPrompt

window.addEventListener('beforeinstallprompt', e => {
  e.preventDefault()
  deferredPrompt = e
})

btnInstallApp.addEventListener('click', e => {
  deferredPrompt.prompt()
  deferredPrompt.userChoice
  .then(choiceResult => {
    if(choiceResult.outcome === 'accepted') {
      console.log('user accepted A2HS prompt')
    } else {
      console.log('user dismissed A2HS prompt')
    }
    deferredPrompt = null
  })
})

document.getElementById('copyUrl').addEventListener('click', function(){
  var textarea = document.createElement('textarea');
  textarea.textContent = "https://www.recomendoapp.com/";
  document.body.appendChild(textarea);
  var selection = document.getSelection();
  var range = document.createRange();
  range.selectNode(textarea);
  selection.removeAllRanges();
  selection.addRange(range);
  console.log('copy success', document.execCommand('copy'));
